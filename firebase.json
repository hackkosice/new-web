{
  "hosting": {
    "redirects": [
      {
        "source": "/apply",
        "destination": "https://apply.hackkosice.com",
        "type": 302
      },
      {
        "source": "/lh-2020/zoom",
        "destination": "https://zoom.us/j/96637836216",
        "type": 302
      },
      {
        "source": "/feedback",
        "destination": "https://docs.google.com/forms/d/e/1FAIpQLSeLNYdsCnGKKLbSwFirlBxUHjnaKnyrOe6av2VzyGU50nv-hw/viewform?usp=pp_url&entry.2071324479=Gymn%C3%A1zium,+Po%C5%A1tov%C3%A1+9,+Ko%C5%A1ice",
        "type": 302
      },
      {
        "source": "/marathonproposal",
        "destination": "https://drive.google.com/file/d/1rWkW0vuh6jZC1PgWabY33dlcyuj9Rgr9/view?usp=sharing",
        "type": 302
      },
      {
        "source": "/discord",
        "destination": "https://discord.gg/VG8B8qfeNV",
        "type": 302
      }
    ],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|svg)",
        "headers": [
          {
            "key": "Content-Encoding",
            "value": "gzip"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=86400"
          }
        ]
      },
      {
        "source": "**/*_hu*.@(jpg|jpeg|gif|png|svg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          }
        ]
      },
      {
        "source": "**/*.@(js|css|woff|woff2)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          }
        ]
      }
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  }
}
